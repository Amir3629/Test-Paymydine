{
  "name": "PayMyDine NextJS Theme",
  "type": "A modern Next.js-powered theme for PayMyDine",
  "description": "A modern Next.js-powered theme for PayMyDine",
  "license": "MIT",
  "authors": [
    {
      "name": "Amir"
    }
  ],
  "keywords": [
    "paymydine",
    "theme"
  ],
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "pestphp/pest": "^1.20"
  },
  "extra": {
    "tastyigniter-theme": {
      "code": "paymydine-nextjs",
      "name": "PayMyDine NextJS Theme",
      "locked": true,
      "require": {
        "igniter.cart": "*",
        "igniter.coupons": "*",
        "igniter.frontend": "*",
        "igniter.local": "*",
        "igniter.pages": "*",
        "igniter.payregister": "*",
        "igniter.reservation": "*",
        "igniter.user": "*"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
