+    public function show($orderId)
+    {
+        try {
+            $order = DB::table('ti_orders')
+                ->leftJoin('ti_tables', 'ti_orders.table_id', '=', 'ti_tables.table_id')
+                ->leftJoin('ti_statuses', 'ti_orders.status_id', '=', 'ti_statuses.status_id')
